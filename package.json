{
  "name": "shadow-map",
  "displayName": "Shadow Map - Code Visualizer",
  "description": "Visualize TypeScript/React codebases with comprehensive dependency graphs, component mapping, and AI-ready context export",
  "version": "2.0.0",
  "publisher": "shadowskeep",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShadowsKeep/shadow-map"
  },
  "bugs": {
    "url": "https://github.com/ShadowsKeep/shadow-map/issues"
  },
  "homepage": "https://github.com/ShadowsKeep/shadow-map#readme",
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Visualization",
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "visualizer",
    "dependency-graph",
    "code-map",
    "typescript",
    "react",
    "components",
    "architecture",
    "ai-context",
    "code-analysis",
    "ast"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "shadow-map.start",
        "title": "Shadow Map: Open Visualizer",
        "category": "Shadow Map"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run build:webview",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "test": "npm run compile && node ./node_modules/mocha/bin/_mocha",
    "install:all": "npm install && cd webview-ui && npm install",
    "build:webview": "cd webview-ui && npm run build"
  },
  "dependencies": {
    "dagre": "^0.8.5",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.53",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^2.22.0"
  }
}